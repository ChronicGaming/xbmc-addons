title=zdf.de
target="assetUrl": *"([^"]+)"
url=%s&bw=isdn&pp=wmp
ext_target=href *= *"([^"]+)"
info=Modem- oder ISDN-Verbindung
extension=asf
quality=low
target="assetUrl": *"([^"]+)"
url=%s&bw=dsl1000&pp=wmp
ext_target=href *= *"([^"]+)"
info=DSL- oder Breitband-Verbindung
extension=asf
quality=standard
target="assetUrl": *"([^"]+)"
url=%s&bw=dsl2000&pp=wmp
ext_target=href *= *"([^"]+)"
info=ab DSL-2000
extension=asf
quality=high
########################################################
title=alphacentauri.de
target=player\.avaible\_url\['flashmedia'\]\['1'\] = "(http://[^"]+)";
url=%s
extension=flv
quality=standard
target=player\.avaible\_url\['flashmedia'\]\['2'\] = "(http://[^"]+)";
