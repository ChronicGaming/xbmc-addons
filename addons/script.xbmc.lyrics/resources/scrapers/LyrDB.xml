<scraper title="LyrDB">
    <url address="http://www.lyrdb.com/karaoke/" useragent="Mozilla/4.0 (compatible; MSIE 8.0; Windows NT 5.1; Trident/4.0; .NET CLR 1.1.4322; .NET CLR 2.0.50727)">
        <song join="+" space="+" clean="true" case="lower" search="?action=search&amp;q=" urlencode="false">
            <artist tail="" sub="false" group=""/>
            <title tail=""/>
        </song>
    </url>
    <source encoding="utf-8">
        <lyrics type="lrc" clean="true">
            <regex flags="dotall ignorecase">&lt;p.+?style=&quot;font-family:[^&quot;]+&quot;&gt;(.+?)&lt;/p&gt;</regex>
        </lyrics>
        <songlist swap="true" always="true" select="auto">
            <regex flags="ignorecase">&lt;tr&gt;&lt;td.+?class=&quot;tresults&quot;&gt;&lt;a.+?href=&quot;/karaoke/([0-9]+.htm)&quot;&gt;(.*?)&lt;/td&gt;&lt;td.+?class=&quot;tresults&quot;&gt;(.*?)&lt;/td&gt;</regex>
        </songlist>
    </source>
</scraper>
