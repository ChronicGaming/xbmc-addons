<scraper title="LyricWiki-Gracenote">
    <url address="http://lyrics.wikia.com/Gracenote:" useragent="Mozilla/4.0 (compatible; MSIE 8.0; Windows NT 5.1; Trident/4.0; .NET CLR 1.1.4322; .NET CLR 2.0.50727)">
        <song join=":" space="_" clean="false" case="title" search="" urlencode="false">
            <artist tail="" sub="false" group=""/>
            <title tail=""/>
        </song>
    </url>
    <source encoding="UTF-8">
        <lyrics type="standard" clean="true">
            <regex flags="dotall ignorecase">\(uneditable Gracenote version\).+?&lt;div.+?class='lyricbox'&gt;.+?&lt;p&gt;(.+?)&lt;!--</regex>
        </lyrics>
        <songlist swap="true" always="false" select="manual">
            <regex flags="ignorecase">&lt;a.+?href=&quot;/Gracenote:([^&quot;]+)&quot;.+?title=&quot;[^&quot;]+&quot;&gt;([^\&lt;]+)&lt;/a&gt;</regex>
        </songlist>
    </source>
</scraper>
